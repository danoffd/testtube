<!-- tt_tree_branch displays a single list of nodes.  It may add nodes
that need to recursively include other branches-->
<ul class="tree-outer tree-node-list">
    <li>
        <div class="tree-root-dropzone" >
            <div class="tree-node-nav"></div>
        </div>
    </li>
    <% nodes.each do |node| %>
        <%= render(node) %>
    <% end %>
</ul>
      
